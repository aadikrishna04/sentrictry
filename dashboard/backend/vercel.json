{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30
    },
    "api/test.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/test",
      "destination": "/api/test.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
